  <thead>
    <tr>
      <th width="50%">{{ 'admin.dictionary.name'|trans }}</th>
      <th width="15%">{{ 'admin.dictionary.serial_number'|trans }}</th>
      <th width="20%">{{ 'admin.dictionary.operation'|trans }}</th>
