{% extends 'old-bootstrap-modal-layout.html.twig' %}


{% block title %}{{'admin.operation_invite.modal.detail_title'|trans}}{% endblock %}

{% block body %}

  <table class="table table-hover table-striped" >
  <thead>
  	<tr>
  	<th>{{'admin.operation_invite.nickname_th'|trans}}</th>
  	<th>{{'admin.operation_invite.modal.totalPrice_th'|trans}}</th>
    <th>{{'admin.operation_invite.modal.coinAmountTotalPrice_th'|trans}}</th>
    <th>{{'admin.operation_invite.modal.amountTotalPrice_th'|trans}}</th>
  	</tr>
  </thead>
  <tbody>
  	{% for detail in details %}
		  <tr>
  		<td>{{detail.nickname}}</td>
  		<td>{{detail.totalPrice|default('0')}}</td>
      <td>{{detail.coinAmountTotalPrice|default('0')}}</td>
      <td>{{detail.amountTotalPrice|default('0')}}</td>
  		</tr>
  	{% endfor %}	
  </tbody>
  </table>
    

{% endblock %}