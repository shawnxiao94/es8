{% extends 'my/money-record/layout.html.twig' %}

{% set main_nav = 'PayoutRecords' %}

{% block main_body %}
  <table class="table table-striped table-hover" id="payouts-table">
    <thead>
      <th>{{'my.money_record.payout.name'|trans}}</th>
      <th>{{'my.money_record.payout_money'|trans}}</th>
      <th>{{'my.money_record.payout_time'|trans}}</th>
    </thead>
    <tbody>
      {% for payoutRecord in payoutRecords %}
        <tr>
          <td>{{ payoutRecord.title }}</td>
          <td>{{ payoutRecord.amount }}</td>
          <td>{{ payoutRecord.transactionTime }}</td>
        </tr>
      {% else %}
        <tr>
          <td colspan="20" class="empty">{{'my.money_record.payout_empty'|trans}}</td>
        </tr>
      {% endfor %}
    </tbody>
  </table>
    {{ web_macro.paginator(paginator) }}
{% endblock %}